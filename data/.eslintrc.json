{
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
        "expect": true
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    }
}
